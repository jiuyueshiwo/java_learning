package com.xiaodai;

import java.time.*;

public class Food {
	private LocalDate startdate;
	private LocalDate enddate;
	private int shelflife; 
    public Food(LocalDate date, int period) {
    	startdate = date;
    	shelflife = period;
    	enddate =startdate.plusDays(shelflife);
    	System.out.println(enddate.toString()+" "+shelflife);
		
	}
}
